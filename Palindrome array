#include <stdio.h>

int main()
{
    int arr[100], rev_arr[100];
    int i, j, n, is_palindrome = 1;

    printf("Enter size of array: ");
    scanf("%d", &n);

    printf("Enter elements of array: ");
    for(i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    // reverse the array
    for(i = n-1, j = 0; i >= 0; i--, j++)
    {
        rev_arr[j] = arr[i];
    }

    // check if it's a palindrome array
    for(i = 0; i < n; i++)
    {
        if(arr[i] != rev_arr[i])
        {
            is_palindrome = 0;
            break;
        }
    }

    if(is_palindrome)
    {
        printf("The array is a palindrome array.\n");
    }
    else
    {
        printf("The array is not a palindrome array.\n");
    }

    return 0;
}
